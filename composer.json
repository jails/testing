{
    "name": "crysalead/testing",
    "description": "End-to-End Functional Testing With Kahlan",
    "keywords": ["Test", "Functional Testing", "Kahlan", "Mink"],
    "license": "MIT",
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "crysalead/kahlan": "~2.0",
        "behat/mink": "~1.7",
        "symfony/css-selector" : "2.7",
        "behat/mink-selenium2-driver": "~1.3",
        "peridot-php/webdriver-manager": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Testing\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
